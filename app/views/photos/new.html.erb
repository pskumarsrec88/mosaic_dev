<% if @profile_picture.present? %>
<h1>Profile</h1>
<%= image_tag @profile_picture+"?width=200&height=200"%>
<%end%>
<% if @cover_picture.present? %>
<h1>cover</h1>
<%= image_tag @cover_picture %>
<%end%>
<h1>Friends</h1>
<% @friend_photo.each do |friend_photo| %>
	<%= image_tag friend_photo+"?width=100&height=100"%>
<%end%>
<h1>photo Tags</h1>
<% @photo.each do |photo| %>
	<%= image_tag photo%>
<%end%>
<h1>Photo uploaded</h1>
<% @album.each do |album| %>
	<%= image_tag album%>
<%end%>